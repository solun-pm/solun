# Copy this file to .env and fill in the values before running docker compose.

# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_SECRET=change-me-to-a-random-64-char-hex-string

# Public URL of the frontend (used by the API for CORS)
FRONTEND_URL=http://localhost:3000

# Public URL of the API (used by the Next.js frontend)
NEXT_PUBLIC_API_URL=http://localhost:3001
